<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans  
                    http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">


	 <bean id="data" class="in.happycoding.bean.Date" primary="true">
	
	</bean>
	
	<bean id="address" class="in.happycoding.bean.Address">
	
	</bean>
	
	<bean id="employee" class="in.happycoding.bean.Employee" autowire="byType">
		
	</bean> 
	
	<bean id="birthDate" class="in.happycoding.bean.Date">
	
	</bean>
	
	<bean id="admissionDate" class="in.happycoding.bean.Date">
	
	</bean>

	<bean id="student" class="in.happycoding.bean.Student" autowire="byName">
	
	</bean>

</beans>  














